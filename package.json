{
  "name": "frontend-appapplication",
  "version": "0.0.1",
  "description": "The frontend appapplication",
  "main": "index.js",
  "scripts": {
    "build:spa": "cross-env SPA=1 webpack --config ./build/webpack.js",
    "build:ssr": "cross-env SSR=1 webpack --config ./build/webpack.js",
    "build:ssr:server": "webpack --config ./build/webpack.ssr.server.js",
    "build:ssr:client": "webpack --config ./build/webpack.ssr.client.js",
    "spa": "npm run build:spa && cross-env SPA=1 node ./server.js",
    "ssr": "npm run build:ssr && cross-env SSR=1 node ./server.js",
    "dev": "cross-env SPA=1 webpack-dev-server --content-base dist --config ./build/webpack.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "sashmlv",
  "license": "ISC",
  "devDependencies": {
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "mini-css-extract-plugin": "^1.0.0",
    "page": "^1.11.6",
    "remove-files-webpack-plugin": "^1.4.4",
    "svelte": "^3.0.0",
    "svelte-loader": "^2.13.6",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "bootstrap": "^5.0.0-alpha2"
  }
}
