{
  "name": "frontend-appapplication",
  "version": "0.0.1",
  "description": "The frontend appapplication",
  "main": "index.js",
  "scripts": {
    "build:spa": "cross-env SPA=true webpack --config ./build/webpack",
    "build:ssr": "cross-env SSR=true webpack --config ./build/webpack",
    "build:ssr:server": "webpack --config ./build/webpack.ssr.server",
    "build:ssr:client": "webpack --config ./build/webpack.ssr.client",
    "spa": "npm run build:spa && http-server -p 3000 dist",
    "ssr": "npm run build:ssr && node ./server",
    "dev": "cross-env SPA=1 webpack-dev-server --content-base dist --config ./build/webpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "sashmlv",
  "license": "ISC",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "http-server": "^0.12.3",
    "mini-css-extract-plugin": "^1.0.0",
    "style-loader": "^0.23.1",
    "svelte": "^3.0.0",
    "svelte-loader": "^2.13.6",
    "svelte-routing": "^1.4.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "bootstrap": "^5.0.0-alpha2"
  }
}
